{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite serve --mode development",
    "dev:sit": "vite serve --mode production:sit",
    "dev:uat": "vite serve --mode production:uat",
    "dev:production": "vite serve --mode production",
    "build": "vue-tsc --noEmit && vite build --mode production",
    "build:sit": "vue-tsc --noEmit && vite build --mode production:sit",
    "build:uat": "vue-tsc --noEmit && vite build --mode production:uat",
    "preview": "vite preview",
    "eslint:comment": "使用 ESLint 检查并自动修复 src 目录下所有扩展名为 .js 和 .vue 的文件",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "prettier:comment": "自动格式化当前目录下的所有文件",
    "prettier": "prettier --write"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@vueuse/core": "^9.3.1",
    "axios": "^1.1.3",
    "echarts": "^5.4.1",
    "element-plus": "^2.2.26",
    "js-md5": "^0.7.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "pinia-plugin-persistedstate": "^3.0.2",
    "qs": "^6.11.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  }
}